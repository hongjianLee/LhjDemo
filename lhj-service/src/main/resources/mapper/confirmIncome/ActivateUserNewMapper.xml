<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lhj.confirmIncome.mapper.ActivateUserNewMapper">

    <select id="getMonths" resultType="string" parameterType="string">
        SELECT
            month_time
        FROM
            activate_user_new
        WHERE
              app_id = #{appId}
        GROUP BY
            month_time
    </select>
</mapper>
